{
  "title": "Hunspell.NetCore",
  "description": "A .NET port of Hunspell",
  "version": "0.0.0-*",
  "packOptions": {
    "summary": "A .NET port of Hunspell",
    "tags": [
      "spell",
      "check",
      "spell-check",
      "hunspell"
    ],
    "owners": [
      "aarondandy"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/aarondandy/Hunspell.NetCore"
    },
    "licenseUrl": "https://github.com/aarondandy/Hunspell.NetCore/blob/master/license.txt",
    "projectUrl": "https://github.com/aarondandy/Hunspell.NetCore/",
    "files": {
      "exclude": [
        "**/*.targets"
      ]
    },
    "requireLicenseAcceptance": false
  },
  "buildOptions": {
    "compile": {
      "includeFiles": [
        "../../SharedAssemblyInfo.cs"
      ],
      "excludeFiles": [
        "Hunspell.NetCore.nuget.targets"
      ]
    },
    "define": [
      "TRACE"
    ],
    "xmlDoc": true,
    "nowarn": [
      "CS1591"
    ]
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [
          "DEBUG"
        ],
        "optimize": false
      }
    },
    "Release": {
      "buildOptions": {
        "define": [
          "RELEASE"
        ],
        "optimize": true,
        "warningsAsErrors": true
      }
    }
  },
  "frameworks": {
    ".NETPortable,Version=v4.5,Profile=Profile7": {
      "buildOptions": {
        "define": [
          "PCL",
          "NETSTANDARD_1_1",
          "NO_COMPILED_REGEX",
          "NO_IO_FILE",
          "NO_UTF32",
          "NO_ARRAY_EMPTY"
        ]
      },
      "dependencies": {
        "System.Collections": "4.0.11",
        "System.Globalization": "4.0.11",
        "System.IO":  "4.1.0",
        "System.Linq": "4.1.0",
        "System.Runtime": "4.1.0",
        "System.Runtime.Extensions": "4.1.0",
        "System.Text.Encoding": "4.0.11",
        "System.Threading.Tasks": "4.0.11",
        "System.Text.Encoding.Extensions": "4.0.11",
        "System.Text.RegularExpressions": "4.1.0"
      }
    },
    "net35": {
      "buildOptions": {
        "define": [
          "PRE_NETSTANDARD",
          "NET_3_5",
          "NET_FULL",
          "NO_ASYNC"
        ]
      }
    },
    "net451": {
      "buildOptions": {
        "define": [
          "NETSTANDARD_1_2",
          "NET_4_5_1",
          "NET_FULL",
          "HAS_CODECONTRACTS"
        ]
      }
    },
    "net461": {
      "buildOptions": {
        "define": [
          "NETSTANDARD_1_4",
          "NET_4_6_1",
          "NET_FULL",
          "HAS_CODECONTRACTS"
        ]
      }
    },
    "netstandard1.1": {
      "buildOptions": {
        "define": [
          "NETCORE",
          "NETSTANDARD_1_1",
          "NO_COMPILED_REGEX",
          "NO_IO_FILE",
          "NO_UTF32",
          "NO_ARRAY_EMPTY"
        ]
      },
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    },
    "netstandard1.3": {
      "buildOptions": {
        "define": [
          "NETCORE",
          "NETSTANDARD_1_3"
        ]
      },
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.IO.FileSystem": "4.0.1"
      }
    }
  },
  "dependencies": {}
}